{
  "name": "bytic/locale-detector",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/locale-detector",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Locale\\Detector\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Locale\\Detector\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.0|^8.0",
    "psr/http-message": "^1.0",
    "symfony/intl": "^4.1|^5.0|^6.0",
    "symfony/http-foundation": "^5.0|^6.0",
    "league/pipeline": "^1.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
